{
  "extends": "./configs/base.tsconfig",
  "compilerOptions": {
    "skipLibCheck": false,
    "baseUrl": ".",
    "paths": {
      "@devpodio/application-package": [
        "dev-packages/application-package/src"
      ],
      "@devpodio/application-package/lib/*": [
        "dev-packages/application-package/src/*"
      ],
      "@devpodio/core": [
        "packages/core/src/common"
      ],
      "@devpodio/core/lib/*": [
        "packages/core/src/*"
      ],
      "@devpodio/extension-manager/lib/*": [
        "packages/extension-manager/src/*"
      ],
      "@devpodio/filesystem/lib/*": [
        "packages/filesystem/src/*"
      ],
      "mv": [
        "packages/filesystem/src/typings/mv"
      ],
      "trash": [
        "packages/filesystem/src/typings/trash"
      ],
      "@devpodio/navigator/lib/*": [
        "packages/navigator/src/*"
      ],
      "@devpodio/preferences/lib/*": [
        "packages/preferences/src/*"
      ],
      "@devpodio/workspace/lib/*": [
        "packages/workspace/src/*"
      ],
      "@devpodio/terminal/lib/*": [
        "packages/terminal/src/*"
      ],
      "@devpodio/languages/lib/*": [
        "packages/languages/src/*"
      ],
      "@devpodio/editor/lib/*": [
        "packages/editor/src/*"
      ],
      "@devpodio/mini-browser/lib/*": [
        "packages/mini-browser/src/*"
      ],
      "@devpodio/monaco/lib/*": [
        "packages/monaco/src/*"
      ],
      "@devpodio/cpp/lib/*": [
        "packages/cpp/src/*"
      ],
      "@devpodio/java/lib/*": [
        "packages/java/src/*"
      ],
      "@devpodio/java-debug/lib/*": [
        "packages/java-debug/src/*"
      ],
      "@devpodio/debug/lib/*": [
        "packages/debug/src/*"
      ],
      "@devpodio/debug-nodejs/lib/*": [
        "packages/debug-nodejs/src/*"
      ],
      "@devpodio/python/lib/*": [
        "packages/python/src/*"
      ],
      "@devpodio/git/lib/*": [
        "packages/git/src/*"
      ],
      "@devpodio/process/lib*": [
        "packages/process/src/*"
      ],
      "@devpodio/markers/lib/*": [
        "packages/markers/src/*"
      ],
      "@devpodio/outline-view/lib/*": [
        "packages/outline-view/src/*"
      ],
      "@devpodio/messages/lib/*": [
        "packages/messages/src/*"
      ],
      "@devpodio/merge-conflicts/lib/*": [
        "packages/merge-conflicts/src/*"
      ],
      "@devpodio/output/lib/*": [
        "packages/output/src/*"
      ],
      "@devpodio/callhierarchy/lib/*": [
        "packages/callhierarchy/src/*"
      ],
      "@devpodio/variable-resolver/lib/*": [
        "packages/variable-resolver/src/*"
      ],
      "@devpodio/editorconfig/lib/*": [
        "packages/editorconfig/src/*"
      ],
      "@devpodio/bunyan/lib/*": [
        "packages/bunyan/src/*"
      ],
      "@devpodio/console/lib/*": [
        "packages/console/src/*"
      ],
      "@devpodio/getting-started/lib/*": [
        "packages/getting-started/src/*"
      ],
      "@devpodio/vue/lib/*": [
        "packages/vue/src/*"
      ]
    },
    "plugins": [
      {
        "name": "typescript-tslint-plugin"
      }
    ]
  },
  "include": [
    "dev-packages/*/src",
    "packages/*/src",
    "examples/*/test"
  ]
}
