{
  "private": true,
  "name": "parent",
  "version": "0.0.0",
  "engines": {
    "yarn": "1.0.x || >=1.2.1",
    "node": ">=8.9.3"
  },
  "resolution": {
    "**/@types/node": "8.10.20"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/chai-string": "^1.4.0",
    "@types/jsdom": "^11.0.4",
    "@types/mocha": "^2.2.41",
    "@types/node": "8.10.20",
    "@types/sinon": "^2.3.5",
    "@types/temp": "^0.8.29",
    "@types/webdriverio": "^4.7.0",
    "chai": "^4.1.0",
    "chai-string": "^1.4.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.2.0",
    "electron-mocha": "~3.5.0",
    "ignore-styles": "^5.0.1",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^11.5.1",
    "lerna": "^2.2.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "remap-istanbul": "^0.13.0",
    "rimraf": "^2.6.1",
    "sinon": "^7.2.2",
    "temp": "^0.8.3",
    "ts-node": "<7.0.0",
    "tslint": "^5.12.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.3",
    "typescript-tslint-plugin": "^0.2.1",
    "uuid": "^3.1.0",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-selenium-standalone-service": "0.0.11",
    "wdio-spec-reporter": "0.1.5",
    "webdriverio": "4.14.1"
  },
  "scripts": {
    "prepare": "yarn prepare:travis && yarn prepare:hoisting && yarn rebuild:clean && yarn build:clean",
    "prepare:travis": "node scripts/prepare-travis",
    "prepare:hoisting": "cross-env THEIA_CHECK_HOISTING_NO_BAIL=true ts-node scripts/check-hoisting.ts",
    "build": "run build",
    "build:clean": "run prepare",
    "docs": "run docs \"@devpodio/!(example-)*\"",
    "test": "yarn test:theia && yarn test:electron && yarn test:browser",
    "test:theia": "run test \"@devpodio/!(example-)*\" --stream --concurrency=1",
    "test:browser": "yarn rebuild:browser && run test \"@devpodio/example-browser\"",
    "test:electron": "yarn rebuild:electron && run test \"@devpodio/example-electron\"",
    "rebuild:clean": "rimraf .browser_modules",
    "rebuild:browser": "theia rebuild:browser",
    "rebuild:electron": "theia rebuild:electron",
    "rebuild:electron:debug": "DEBUG=electron-rebuild && yarn rebuild:electron",
    "watch": "lerna run watch --scope \"@devpodio/!(example-)*\" --parallel",
    "publish": "yarn &&  yarn publish:latest",
    "publish:latest": "lerna publish --registry=https://npm.devpod.io/ --skip-git --force-publish",
    "publish:next": "lerna publish --registry=https://npm.devpod.io/ --exact --canary=next --npm-tag=next --force-publish --skip-git --yes"
  },
  "workspaces": [
    "dev-packages/*",
    "packages/*",
    "examples/*"
  ],
  "dependencies": {}
}
