{
  "name": "@devpodio/cpp",
  "version": "0.5.2",
  "description": "Theia - Cpp Extension",
  "dependencies": {
    "@devpodio/core": "^0.5.2",
    "@devpodio/editor": "^0.5.2",
    "@devpodio/filesystem": "^0.5.2",
    "@devpodio/languages": "^0.5.2",
    "@devpodio/monaco": "^0.5.2",
    "@devpodio/preferences": "^0.5.2",
    "@devpodio/process": "^0.5.2",
    "@devpodio/task": "^0.5.2",
    "string-argv": "^0.1.1",
    "snyk": "^1.230.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/cpp-frontend-module",
      "backend": "lib/node/cpp-backend-module"
    }
  ],
  "keywords": [
    "theia-extension"
  ],
  "license": "EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/devpodio/devpodio.git"
  },
  "bugs": {
    "url": "https://github.com/devpodio/devpodio/issues"
  },
  "homepage": "https://github.com/devpodio/devpodio",
  "files": [
    "data",
    "lib",
    "src"
  ],
  "scripts": {
    "prepare": "npm run snyk-protect && yarn run clean && yarn run build",
    "clean": "theiaext clean",
    "build": "theiaext build",
    "watch": "theiaext watch",
    "test": "theiaext test",
    "snyk-protect": "snyk protect"
  },
  "devDependencies": {
    "@devpodio/ext-scripts": "^0.5.2"
  },
  "nyc": {
    "extends": "../../configs/nyc.json"
  },
  "snyk": true
