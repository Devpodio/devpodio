{
  "name": "@devpodio/vue",
  "description": "Vuejs support for the Theia IDE",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "theia-extension"
  ],
  "license": "Apache-2.0",
  "version": "0.4.3",
  "files": [
    "data",
    "lib",
    "src",
    "languages"
  ],
  "dependencies": {
    "@devpodio/callhierarchy": "^0.4.3",
    "@devpodio/core": "^0.4.3",
    "@devpodio/languages": "^0.4.3",
    "@devpodio/monaco": "^0.4.3",
    "@devpodio/process": "^0.4.3",
    "@devpodio/vue-language-server": "^0.0.48"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "typescript": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "theiaext clean",
    "build": "theiaext build",
    "watch": "theiaext watch",
    "test": "theiaext test",
    "docs": "theiaext docs"
  },
  "author": "Uni Sayo <unibtc@gmail.com> (https://github.com/devpodio)",
  "repository": "https://github.com/devpodio/devpodio",
  "bugs": {
    "url": "https://github.com/Devpodio/devpodio/issues"
  },
  "homepage": "https://github.com/Devpodio/devpodio#readme",
  "nyc": {
    "extends": "../../configs/nyc.json"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/vue-frontend-module",
      "backend": "lib/node/vue-backend-module"
    }
  ]
}
