{
  "private": true,
  "name": "@devpodio/example-browser",
  "version": "1.0.14",
  "theia": {
    "frontend": {
      "config": {
        "applicationName": "Theia Browser Example"
      }
    }
  },
  "dependencies": {
    "@devpodio/callhierarchy": "^1.0.14",
    "@devpodio/console": "^1.0.14",
    "@devpodio/core": "^1.0.14",
    "@devpodio/cpp": "^1.0.14",
    "@devpodio/debug": "^1.0.14",
    "@devpodio/debug-nodejs": "^1.0.14",
    "@devpodio/editor": "^1.0.14",
    "@devpodio/editor-preview": "^1.0.14",
    "@devpodio/editorconfig": "^1.0.14",
    "@devpodio/extension-manager": "^1.0.14",
    "@devpodio/file-search": "^1.0.14",
    "@devpodio/filesystem": "^1.0.14",
    "@devpodio/getting-started": "^1.0.14",
    "@devpodio/git": "^1.0.14",
    "@devpodio/java": "^1.0.14",
    "@devpodio/java-debug": "^1.0.14",
    "@devpodio/json": "^1.0.14",
    "@devpodio/keymaps": "^1.0.14",
    "@devpodio/languages": "^1.0.14",
    "@devpodio/markers": "^1.0.14",
    "@devpodio/merge-conflicts": "^1.0.14",
    "@devpodio/messages": "^1.0.14",
    "@devpodio/metrics": "^1.0.14",
    "@devpodio/mini-browser": "^1.0.14",
    "@devpodio/monaco": "^1.0.14",
    "@devpodio/navigator": "^1.0.14",
    "@devpodio/outline-view": "^1.0.14",
    "@devpodio/output": "^1.0.14",
    "@devpodio/plugin-ext": "^1.0.14",
    "@devpodio/plugin-ext-vscode": "^1.0.14",
    "@devpodio/preferences": "^1.0.14",
    "@devpodio/preview": "^1.0.14",
    "@devpodio/process": "^1.0.14",
    "@devpodio/python": "^1.0.14",
    "@devpodio/search-in-workspace": "^1.0.14",
    "@devpodio/task": "^1.0.14",
    "@devpodio/terminal": "^1.0.14",
    "@devpodio/textmate-grammars": "^1.0.14",
    "@devpodio/tslint": "^1.0.14",
    "@devpodio/typescript": "^1.0.14",
    "@devpodio/userstorage": "^1.0.14",
    "@devpodio/variable-resolver": "^1.0.14",
    "@devpodio/vue": "^1.0.14",
    "@devpodio/workspace": "^1.0.14"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn build",
    "clean": "theia clean && rimraf errorShots",
    "build": "theia build --env.hashed",
    "watch": "yarn build --watch",
    "start": "export THEIA_DEFAULT_PLUGINS=local-dir:../../plugins && theia start --startup-timeout -1 --log-config ./logger.config.json --port 3030 --hostname 0.0.0.0",
    "start:debug": "yarn start --log-level=debug",
    "test": "wdio wdio.conf.js",
    "test-non-headless": "wdio wdio-non-headless.conf.js",
    "coverage:compile": "yarn build --config coverage-webpack.config.js",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage-final.json --exclude 'frontend/index.js' && rimraf coverage/coverage.json",
    "coverage:report:html": "istanbul report --root coverage --format html",
    "coverage:report:lcov": "istanbul report --root coverage --format lcov",
    "coverage": "yarn coverage:compile && yarn test && yarn coverage:remap && yarn coverage:report:lcov && yarn coverage:report:html"
  },
  "devDependencies": {
    "@devpodio/cli": "^1.0.14"
  }
}
