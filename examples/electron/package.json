{
  "private": true,
  "name": "@devpodio/example-electron",
  "version": "0.5.0",
  "license": "EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0",
  "theia": {
    "target": "electron",
    "frontend": {
      "config": {
        "applicationName": "Theia Electron Example"
      }
    }
  },
  "//": [
    "`@devpodio/plugin-ext` and `@devpodio/plugin-ext-vscode` are missing due to https://github.com/theia-ide/theia/issues/3723",
    "`@devpodio/debug`, `@devpodio/debug-nodejs` and `@devpodio/java-debug` are removed due to https://github.com/theia-ide/theia/issues/3716"
  ],
  "dependencies": {
    "@devpodio/callhierarchy": "^0.5.0",
    "@devpodio/console": "^0.5.0",
    "@devpodio/core": "^0.5.0",
    "@devpodio/cpp": "^0.5.0",
    "@devpodio/debug": "^0.5.0",
    "@devpodio/debug-nodejs": "^0.5.0",
    "@devpodio/editor": "^0.5.0",
    "@devpodio/editor-preview": "^0.5.0",
    "@devpodio/editorconfig": "^0.5.0",
    "@devpodio/extension-manager": "^0.5.0",
    "@devpodio/file-search": "^0.5.0",
    "@devpodio/filesystem": "^0.5.0",
    "@devpodio/getting-started": "^0.5.0",
    "@devpodio/git": "^0.5.0",
    "@devpodio/java": "^0.5.0",
    "@devpodio/java-debug": "^0.5.0",
    "@devpodio/json": "^0.5.0",
    "@devpodio/keymaps": "^0.5.0",
    "@devpodio/languages": "^0.5.0",
    "@devpodio/markers": "^0.5.0",
    "@devpodio/merge-conflicts": "^0.5.0",
    "@devpodio/messages": "^0.5.0",
    "@devpodio/metrics": "^0.5.0",
    "@devpodio/mini-browser": "^0.5.0",
    "@devpodio/monaco": "^0.5.0",
    "@devpodio/navigator": "^0.5.0",
    "@devpodio/outline-view": "^0.5.0",
    "@devpodio/output": "^0.5.0",
    "@devpodio/preferences": "^0.5.0",
    "@devpodio/preview": "^0.5.0",
    "@devpodio/process": "^0.5.0",
    "@devpodio/python": "^0.5.0",
    "@devpodio/search-in-workspace": "^0.5.0",
    "@devpodio/task": "^0.5.0",
    "@devpodio/terminal": "^0.5.0",
    "@devpodio/textmate-grammars": "^0.5.0",
    "@devpodio/tslint": "^0.5.0",
    "@devpodio/typehierarchy": "^0.5.0",
    "@devpodio/typescript": "^0.5.0",
    "@devpodio/userstorage": "^0.5.0",
    "@devpodio/variable-resolver": "^0.5.0",
    "@devpodio/vue": "^0.5.0",
    "@devpodio/workspace": "^0.5.0"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn build",
    "clean": "theia clean",
    "build": "theia build --mode development",
    "watch": "yarn build --watch",
    "start": "theia start",
    "start:debug": "yarn start --log-level=debug",
    "test": "electron-mocha --timeout 60000 --require ts-node/register \"./test/**/*.espec.ts\"",
    "test:ui": "wdio wdio.conf.js"
  },
  "devDependencies": {
    "@devpodio/cli": "^0.5.0"
  }
}
