{
  "private": true,
  "name": "@devpodio/example-electron",
  "version": "1.0.17",
  "theia": {
    "target": "electron",
    "frontend": {
      "config": {
        "applicationName": "Theia Electron Example"
      }
    }
  },
  "//": [
    "`@devpodio/plugin-ext` and `@devpodio/plugin-ext-vscode` are missing due to https://github.com/theia-ide/theia/issues/3723",
    "`@devpodio/debug`, `@devpodio/debug-nodejs` and `@devpodio/java-debug` are removed due to https://github.com/theia-ide/theia/issues/3716"
  ],
  "dependencies": {
    "@devpodio/callhierarchy": "^1.0.17",
    "@devpodio/console": "^1.0.17",
    "@devpodio/core": "^1.0.17",
    "@devpodio/cpp": "^1.0.17",
    "@devpodio/debug": "^1.0.17",
    "@devpodio/debug-nodejs": "^1.0.17",
    "@devpodio/editor": "^1.0.17",
    "@devpodio/editor-preview": "^1.0.17",
    "@devpodio/editorconfig": "^1.0.17",
    "@devpodio/extension-manager": "^1.0.17",
    "@devpodio/file-search": "^1.0.17",
    "@devpodio/filesystem": "^1.0.17",
    "@devpodio/getting-started": "^1.0.17",
    "@devpodio/git": "^1.0.17",
    "@devpodio/java": "^1.0.17",
    "@devpodio/java-debug": "^1.0.17",
    "@devpodio/json": "^1.0.17",
    "@devpodio/keymaps": "^1.0.17",
    "@devpodio/languages": "^1.0.17",
    "@devpodio/markers": "^1.0.17",
    "@devpodio/merge-conflicts": "^1.0.17",
    "@devpodio/messages": "^1.0.17",
    "@devpodio/metrics": "^1.0.17",
    "@devpodio/mini-browser": "^1.0.17",
    "@devpodio/monaco": "^1.0.17",
    "@devpodio/navigator": "^1.0.17",
    "@devpodio/outline-view": "^1.0.17",
    "@devpodio/output": "^1.0.17",
    "@devpodio/preferences": "^1.0.17",
    "@devpodio/preview": "^1.0.17",
    "@devpodio/process": "^1.0.17",
    "@devpodio/python": "^1.0.17",
    "@devpodio/search-in-workspace": "^1.0.17",
    "@devpodio/task": "^1.0.17",
    "@devpodio/terminal": "^1.0.17",
    "@devpodio/textmate-grammars": "^1.0.17",
    "@devpodio/tslint": "^1.0.17",
    "@devpodio/typescript": "^1.0.17",
    "@devpodio/userstorage": "^1.0.17",
    "@devpodio/variable-resolver": "^1.0.17",
    "@devpodio/workspace": "^1.0.17"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn build",
    "clean": "theia clean",
    "build": "theia build --mode development",
    "watch": "yarn build --watch",
    "start": "theia start",
    "start:debug": "yarn start --log-level=debug",
    "test": "electron-mocha --timeout 60000 --require ts-node/register \"./test/**/*.espec.ts\"",
    "test:ui": "wdio wdio.conf.js"
  },
  "devDependencies": {
    "@devpodio/cli": "^1.0.17"
  }
}
